<?xml version="1.0" encoding="utf-8"?>
<Robot
    name="Rev Scoot"
    type="FirstInspires-FTC-template">
    <LynxUsbDevice
        name="REV Hub"
        parentModuleAddress="2"
        serialNumber="">
        <LynxModule
            name="Expansion Hub"
            port="2">
            <RevRoboticsCoreHexMotor
                name="left_drive"
                port="0" />
            <RevRoboticsCoreHexMotor
                name="right_drive"
                port="1" />
            <RevRoboticsCoreHexMotor
                name="left_arm"
                port="2" />
            <RevRoboticsCoreHexMotor
                name="collector"
                port="3" />
            <Servo
                name="left_hand"
                port="0" />
            <Servo
                name="right_hand"
                port="1" />
            <DigitalDevice
                name="sensor_digital"
                port="0" />
            <LynxEmbeddedIMU
                bus="0"
                name="imu"
                port="0" />
        </LynxModule>
    </LynxUsbDevice>
</Robot>